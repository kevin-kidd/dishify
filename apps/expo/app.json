{
  "expo": {
    "name": "dishify",
    "slug": "dishify",
    "scheme": "dishify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dishify.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dishify.app"
    },
    "extra": {
      "eas": {
        "projectId": "9872f924-faba-46a9-81ab-6592cab621b7"
      }
    },
    "owner": "harrisonkevin",
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "dishify",
          "project": "dishify-mobile",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  }
}
